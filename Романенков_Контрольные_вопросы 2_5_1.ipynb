   "source": [
    "def factorial(x) :\n",
    "    a = 1;\n",
    "    for i in range(1, int(x)+1, 1) :\n",
    "        a = a * i\n",
    "    return a\n",
    "\n",
    "n = input('Введите число: ')\n",
    "\n",
    "print('Factorial n = ', factorial(n))\n",
    "\n"
   ]
